<h1>Listing articles</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th>Cedula</th>
  </tr>

  <% @documents.each do |document| %>
    <tr>
      <td><%= document.title %></td>
      <td><%= document.text %></td>
      <td><%= document.cedula %></td>
      <td><%= document.dato1 %></td>
      
    </tr>
  <% end %>
</table>
